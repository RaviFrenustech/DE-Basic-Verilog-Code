#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010FD728 .scope module, "tb" "tb" 2 2;
 .timescale -9 -11;
v010F32A0_0 .var "Addr", 5 0;
v010FE9E0_0 .net "DataBus", 63 0, v010F3248_0; 1 drivers
v010FEA38_0 .var "MemRd", 0 0;
v010FEA90_0 .var "MemWr", 0 0;
S_00FB20D8 .scope module, "uut" "memory" 2 9, 3 2, S_010FD728;
 .timescale -9 -11;
v00FB2160_0 .net "Addr", 5 0, v010F32A0_0; 1 drivers
v00FB21B8_0 .alias "DataBus", 63 0, v010FE9E0_0;
v010F8900 .array "Mem", 63 0, 63 0;
v010F8958_0 .net "MemRd", 0 0, v010FEA38_0; 1 drivers
v010F31F0_0 .net "MemWr", 0 0, v010FEA90_0; 1 drivers
v010F3248_0 .var "datareg", 63 0;
E_010FCBE0 .event edge, v010F3248_0, v00FB2160_0, v010F8958_0, v010F31F0_0;
    .scope S_00FB20D8;
T_0 ;
    %vpi_call 3 10 "$readmemh", "input.txt", v010F8900;
    %vpi_call 3 11 "$display", $time, "ns data write process done ";
    %end;
    .thread T_0;
    .scope S_00FB20D8;
T_1 ;
    %wait E_010FCBE0;
    %load/v 8, v010F31F0_0, 1;
    %load/v 9, v010F8958_0, 1;
    %cmpi/u 9, 0, 1;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_1.0, 8;
    %load/v 8, v00FB21B8_0, 64;
    %ix/getv 3, v00FB2160_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v010F8900, 8, 64;
t_0 ;
    %set/v v010F3248_0, 3, 64;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v010F8958_0, 1;
    %jmp/0xz  T_1.2, 8;
    %ix/getv 3, v00FB2160_0;
    %load/av 8, v010F8900, 64;
    %set/v v010F3248_0, 8, 64;
    %vpi_call 3 22 "$display", $time, "ns data read Address-%h is %h\012", v00FB2160_0, v010F3248_0;
    %vpi_call 3 25 "$display", $time, "ns data bus data is %h\012", v00FB21B8_0;
    %jmp T_1.3;
T_1.2 ;
    %set/v v010F3248_0, 3, 64;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_010FD728;
T_2 ;
    %set/v v010FEA90_0, 0, 1;
    %set/v v010FEA38_0, 0, 1;
    %set/v v010F32A0_0, 0, 6;
    %delay 2000, 0;
    %set/v v010FEA38_0, 1, 1;
    %set/v v010FEA90_0, 0, 1;
    %set/v v010F32A0_0, 0, 6;
    %delay 1000, 0;
    %vpi_call 2 26 "$display", " Memory is read from Address %h----%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 1, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 29 "$display", " Memory is read from Address %h---%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 2, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 32 "$display", " Memory is read from Address %h---%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 3, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 35 "$display", " Memory is read from Address %h---%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 4, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 38 "$display", " Memory is read from Address %h--%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 5, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 41 "$display", " Memory is read from Address %h--%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 2000, 0;
    %movi 8, 6, 6;
    %set/v v010F32A0_0, 8, 6;
    %delay 1000, 0;
    %vpi_call 2 44 "$display", " Memory is read from Address %h---%h\012", v010F32A0_0, v010FE9E0_0;
    %delay 1000, 0;
    %vpi_call 2 45 "$finish";
    %end;
    .thread T_2;
    .scope S_010FD728;
T_3 ;
    %vpi_call 2 49 "$dumpfile", "dump.vcd";
    %vpi_call 2 50 "$dumpvars";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    ".\tb_mem_ctrol.sv";
    "./mem_ctrol.sv";
